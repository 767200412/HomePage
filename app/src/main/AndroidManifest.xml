<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comdemo.example.dell.homepagedemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Ui.MainPage.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Ui.LoginPage.MainLoginActivity" />
        <activity android:name=".Ui.LoginPage.Register" />
        <activity android:name=".Ui.LoginPage.MessageVerification" />
        <activity android:name=".Ui.LoginPage.Password" />
        <activity android:name=".Ui.LoginPage.ReallyName" />
        <activity android:name=".Ui.LoginPage.VerificationLog" />
        <activity android:name=".Ui.LoginPage.FindPassword" />
        <activity android:name=".Ui.LoginPage.NewPassword"/>
    </application>

</manifest>